// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+down",            "command": "editor.action.moveLinesDownAction",
                                    "when": "editorTextFocus" },
    { "key": "cmd+up",              "command": "editor.action.moveLinesUpAction",
                                    "when": "editorTextFocus" },
    { "key": "cmd+alt+down",        "command": "editor.action.copyLinesDownAction",
                                    "when": "editorTextFocus" },
    { "key": "cmd+alt+up",          "command": "editor.action.copyLinesUpAction",
                                    "when": "editorTextFocus" },
    { "key": "cmd+d",               "command": "editor.action.deleteLines",
                                    "when": "editorTextFocus" },
    { "key": "cmd+w",               "command": "workbench.files.action.closeFile" },
    { "key": "alt+down",            "command": "workbench.files.action.openNextWorkingFile" },
    { "key": "alt+up",              "command": "workbench.files.action.openPreviousWorkingFile" },
    { "key": "cmd+left",            "command": "workbench.action.focusSideBar" },
    { "key": "cmd+right",           "command": "workbench.action.focusFirstEditor" },
    { "key": "alt+cmd+left",        "command": "workbench.action.focusLeftEditor" },
    { "key": "alt+cmd+right",       "command": "workbench.action.focusRightEditor" },
]